@model IEnumerable<webSede.Models.Sede_Complejo>

@{
    ViewBag.Title = "Complejo Deportivo";
}

<h2>Complejo deportivos</h2>

<p>
    @Html.ActionLink("Nuevo Complejo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Tipo 
        </th>
        <th>
            Nombre
        </th>
        <th>
           Area
        </th>
        <th>
           Localización
        </th>
        <th>
            Jefe
        </th>
        <th>
           Sede
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tipo_sede)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.area)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.localizacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Jefe_organizacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sede.nombre)
        </td>
        <td>
            @Html.ActionLink("Modificar", "Edit", new { id=item.id_complejo }) |
            @Html.ActionLink("Ver", "Details", new { id=item.id_complejo }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.id_complejo })
        </td>
    </tr>
}

</table>
